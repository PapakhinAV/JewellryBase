<div class="columns">
  <div class="button_block">
    <a href="/entries/new" class="block button w-100 mar-t-2 mar-b-3 pad-2 round-1 text-c center">Add new Item</a>
  </div>

  <main role="main list">
    <ul class="entries-list no-bullets no-padding">
      {{#each entries}}
      <li class="entry-item pad-b-4">

        <a href="/entries/{{id}}" class="entry-title font-2 pad-b-1-4"> <label> </label> {{nameItems}}</a>

        <div class="entry-title font-2 pad-b-1-4"> <label>Price: </label>{{price}} <span>rub.</span> </div>
        <img class="imgSize" src='{{linkPhoto}}'>

        <p class="entry-stub">{{describe}}</p>
      </li>
      {{/each}}
    </ul>
  </main>
</div>
